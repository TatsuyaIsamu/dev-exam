<p id="notice"><%= notice %></p>

<h1>Basic Informations</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @basic_informations.each do |basic_information| %>
      <tr>
        <td><%= link_to 'Show', basic_information %></td>
        <td><%= link_to 'Edit', edit_basic_information_path(basic_information) %></td>
        <td><%= link_to 'Destroy', basic_information, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Basic Information', new_basic_information_path %>
