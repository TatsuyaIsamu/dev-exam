<p id="notice"><%= notice %></p>
<h1>閲覧</h1>
<p>物件名：<%= @basic_information.building_name %></p>
<p>賃料：<%= @basic_information.rent %></p>
<p>住所：<%= @basic_information.address %>年</p>
<p>築年数：<%= @basic_information.age %></p>
<p>備考：<%= @basic_information.info %></p>
        

<% for number in 0..@basic_information.nearest_stations.length-1 do%>
  <h2>最寄り駅<%= number+1%></h2>
  <p>路線名<%= @basic_information.nearest_stations[number].route_name %></p>
  <p>駅名<%= @basic_information.nearest_stations[number].station_name%></p>
  <p>徒歩分数<%= @basic_information.nearest_stations[number].minuites_on_foot %>分</p>
<% end %>
<%= link_to 'Edit', edit_basic_information_path(@basic_information) %> |
<%= link_to 'Back', basic_informations_path %>
